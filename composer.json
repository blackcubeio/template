{
    "name": "blackcube/template",
    "description": "Blackcube template Application",
    "keywords": ["Blackcube", "template"],
    "homepage": "https://code.redcat.io/blackcube/template",
    "type": "project",
    "license": "proprietary",
    "support": {
        "issues": "https://code.redcat.io/blackcube/template/issues",
        "wiki": "https://code.redcat.io/blackcube/template/wikis/home",
        "source": "https://code.redcat.io/blackcube/template/tree/devel"
    },
    "authors": [
        {
            "name": "Philippe Gaultier",
            "email": "pgaultier@redcat.io"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-gd": "*",
        "blackcube/core": "3.0.x-dev",
        "blackcube/admin": "3.0.x-dev",
        "vlucas/phpdotenv": "~5.0",
        "yiisoft/yii2": "~2.0",
        "yiisoft/yii2-redis": "~2.0"
    },
    "require-dev" : {
        "yiisoft/yii2-debug":"~2.0",
        "yiisoft/yii2-gii":"~2.0"
    },
    "config": {
        "process-timeout": 1800,
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "autoload": {
        "psr-4": {
            "console\\": "console",
            "app\\": "common",
            "webapp\\": "webapp",
            "modules\\": "modules",
            "plugins\\": "plugins"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {
        "post-install-cmd": [
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-devel-3.x": "3.0.x-dev"
        }
    }
}
